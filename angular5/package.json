{
  "name": "utc-angular",
  "version": "1.0.0",
  "private": true,
  "description": "Packages needed by UTC angular components",
  "scripts": {
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "serve": "lite-server -c=bs-config.json",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "build:upgrade": "tsc",
    "serve:upgrade": "http-server",
    "build:cli": "ng build --no-progress",
    "serve:cli": "http-server dist/",
    "build:aot": "ngc -p tsconfig-aot.json && rollup -c rollup-config.js",
    "serve:aot": "lite-server -c bs-config.aot.json",
    "copy-dist-files": "node ./copy-dist-files.js",
    "i18n": "ng-xi18n",
    "clean": "rm -r dist",
    "test-script": "node dist/app/test-scripts.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^9.1.9",
    "@angular/cli": "^9.1.7",
    "@angular/common": "^9.1.9",
    "@angular/compiler": "^9.1.9",
    "@angular/compiler-cli": "^9.1.9",
    "@angular/core": "^9.1.9",
    "@angular/forms": "^9.1.9",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@angular/platform-server": "^9.1.9",
    "@angular/router": "^9.1.9",
    "angular-date-value-accessor": "^1.0.2",
    "core-js": "^3.6.5",
    "reflect-metadata": "^0.1.13",
    "rx-http-request": "^1.2.0",
    "rxjs": "^6.5.5",
    "systemjs": "6.3.2",
    "zone.js": "^0.10.3",
    "rxjs-compat": "^6.5.5"
  },
  "devDependencies": {
    "@types/jasmine": "3.5.10",
    "@types/node": "^14.0.5",
    "concurrently": "^5.2.0",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "http-server": "^0.12.3",
    "jasmine-core": "~3.5.0",
    "jasmine-rx": "0.1.6",
    "karma": "^5.0.9",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "lite-server": "^2.5.4",
    "lodash": "^4.17.15",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.2",
    "typescript": "3.8.3"
  },
  "repository": {}
}
