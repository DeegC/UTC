{
  "name": "utc-angular",
  "version": "1.0.0",
  "private": true,
  "description": "Packages needed by UTC angular components",
  "scripts": {
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "serve": "lite-server -c=bs-config.json",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "pretest": "npm run build",
    "testk": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "test": "jasmine JASMINE_CONFIG_PATH=src/tests/support/jasmine.json",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "build:upgrade": "tsc",
    "serve:upgrade": "http-server",
    "build:cli": "ng build --no-progress",
    "serve:cli": "http-server dist/",
    "build:aot": "ngc -p tsconfig-aot.json && rollup -c rollup-config.js",
    "serve:aot": "lite-server -c bs-config.aot.json",
    "copy-dist-files": "node ./copy-dist-files.js",
    "i18n": "ng-xi18n",
    "clean": "rm -r dist"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
      "@angular/common": "4.3.6",
      "@angular/compiler": "4.3.6",
      "@angular/core": "4.3.6",
      "@angular/forms": "4.3.6",
      "@angular/http": "4.3.6",
      "@angular/platform-browser": "4.3.6",
      "@angular/platform-browser-dynamic": "4.3.6",
      "@angular/router": "4.3.6",
      "core-js": "^2.4.1",
      "rxjs": "5.4.3",
      "rx-http-request": "^1.2.0",
      "systemjs": "0.19.39",
      "zone.js": "^0.8.4",
      "angular-date-value-accessor": "^0.0.2"
  },
  "devDependencies": {
      "concurrently": "^3.0.0",
      "file-loader": "^0.9.0",
      "html-loader": "^0.4.3",
      "http-server": "^0.9.0",
      "jasmine-core": "~2.4.1",
      "karma": "^1.3.0",
      "karma-chrome-launcher": "^2.0.0",
      "karma-cli": "^1.0.1",
      "karma-jasmine": "^1.0.2",
      "karma-jasmine-html-reporter": "^0.2.2",
      "lite-server": "^2.3.0",
      "lodash": "^4.16.4",
      "protractor": "~4.0.14",
      "rimraf": "^2.5.4",
      "tslint": "^3.15.1",
      "typescript": "~2.4.2",
      "@types/node": "^6.0.46",
      "@types/jasmine": "2.5.36",
      "jasmine-rx": "0.1.6"
  },
  "repository": {}
}
